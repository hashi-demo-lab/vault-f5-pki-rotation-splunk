{{ with pkiCert "pki_intermediate/issue/f5demo" "common_name=f5demo.com"}}
[
    {
        "op": "replace",
        "path": "/Common/Shared/webcert/remark",
        "value": "Updated on {{ timestamp }}"
    },
    {
        "op": "replace",
        "path": "/Common/Shared/webcert/certificate",
        "value": "{{ .Data.Cert | toJSON | replaceAll "\"" "" }}"
    },
    {
        "op": "replace",
        "path": "/Common/Shared/webcert/privateKey",
        "value": "{{ .Data.Key | toJSON | replaceAll "\"" "" }}"
    },
    {
        "op": "replace",
        "path": "/Common/Shared/webcert/chainCA",
        "value": "{{ .Data.CA | toJSON | replaceAll "\"" "" }}"
    }
]
{{ end }}
